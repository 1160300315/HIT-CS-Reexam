{
    "title": "哈工大复试·备考笔记",
    "description": "web developing",
    "author": "lzh1995",
    "output.name": "lzh1995",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": "./docs/",
    "structure": {
    	"readme": "readme.md"
    },
    "styles": {
    	"website": "./website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "-highlight",
        "-livereload",
        "search-plus@^0.0.11",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "advanced-emoji@^0.2.1",
        "include-codeblock@^3.0.2",
        "emphasize@^1.1.0",
        "splitter@^0.0.8",
        "tbfed-pagefooter@^0.0.1",
        "chapter-fold",
        "expandable-chapters-small@^0.1.7",
        "sectionx@^3.1.0",
        "local-video@^1.0.1",
        "favicon@^0.0.2",
        "todo@^0.1.3",
        "terminal@^0.3.2",
        "alerts@^0.2.0",
        "flexible-alerts",
        "callouts"
        
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                
               "prism-themes/themes/prism-dracula.css" 
            ]
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "sectionx": {
            "tag": "b"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },
       
        "expandable-chapters-small":{},
        "chapter-fold": {},
        "flexible-alerts": {},
        "callouts":
        {
            "showTypeInHeader": false
        }
    }
}